        "from sklearn.feature_extraction.text import CountVectorizer\n",
        "from sklearn.linear_model import LogisticRegression\n",
        "from sklearn.svm import LinearSVC\n",
        "from sklearn.metrics import f1_score"
      ]
    },
    {
      "cell_type": "code",
      "execution_count": 56,
      "metadata": {},
      "outputs": [
        {
          "name": "stdout",
          "output_type": "stream",
          "text": [
            "F1 Score (BoW + Logistic Regression): 0.375\n"
          ]
        }
      ],
      "source": [
        "# Feature extraction (BoW)\n",
        "vectorizer = CountVectorizer(max_features=5000)\n",
        "X_train = vectorizer.fit_transform(training_set1.text)\n",
        "X_dev = vectorizer.transform(eval_df.text)\n",
        "\n",
        "# Train Logistic Regression\n",
        "model = LogisticRegression(max_iter=1000)\n",
        "model.fit(X_train, training_set1.label)\n",
        "\n",
        "# Evaluate\n",
        "dev_preds = model.predict(X_dev)\n",
        "f1 = f1_score(eval_df.label, dev_preds)\n",
        "print(f\"F1 Score (BoW + Logistic Regression): {f1}\")"
      ]
