        "# Function to shuffle sentences in a text\n",
        "def shuffle_sentences(text):\n",
        "    # Tokenises text into sentences\n",
        "    sentences = sent_tokenize(text)\n",
        "    print(sentences)\n",
        "    \n",
        "    # Shuffle the sentences\n",
        "    random.shuffle(sentences)\n",
